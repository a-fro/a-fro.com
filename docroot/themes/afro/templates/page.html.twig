{#
/**
 * @file display a single page.
 */
#}
<div class="page-wrapper">
  <header  id="header">
    <div class="branding">
      <a class="logo" href="{{ front_page }}" title="{{ 'Home'|t }}" rel="home">a-fro.com</a>
      {% if site_slogan %}
      <div class="slogan">
        {{ site_slogan }}
      </div>
      {% endif %}
    </div>
    {% if page.header %}
      {{ page.header }}
    {% endif %}
  </header>
  <div id="main-content">
      {% if messages %}
        <div id="messages">
         <div class="contain clearfix">
          {{ messages }}
        </div>
      </div>
      {% endif %}
    {% if title %}
    <div id="title">
      <h1 class="title" id="page-title">
        {{ title }}
      </h1>
    </div>
    {% endif %}
    <main id="content"  role="main">
      {% if tabs %}
      <nav class="tabs" role="navigation">
        {{ tabs }}
      </nav>
      {% endif %}
      {{ page.help }}
      {% if action_links %}
      <ul class="action-links">
        {{ action_links }}
      </ul>
      {% endif %}
      {{ page.content }}
    </main>
  </div>
  <footer  id="footer">
    {% if page.footer %}
    {{ page.footer }}
    {% endif %}
  </footer>
</div>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-10899404-5', 'auto');
  ga('send', 'pageview');

</script>

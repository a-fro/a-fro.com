{#
/**
 * @file display a single page.
 */
#}
<div id="page-wrapper">
  <div id="page">
    <header  id="header">
      <div class="branding">
        <a class="logo" href="{{ front_page }}" title="{{ 'Home'|t }}" rel="home">a-fro.com</a>
        {% if site_slogan %}
        <div class="slogan">
          {{ site_slogan }}
        </div>
        {% endif %}
      </div>
      {% if page.nav %}
        {{ page.nav }}
      {% endif %}
    </header>
    <div class="main-content">
      {% if messages %}
          <div id="messages">
              <div class="contain">
                  {{ messages }}
              </div>
          </div>
      {% endif %}
      {% if tabs %}
          <nav class="tabs" role="navigation">
              {{ tabs }}
          </nav>
      {% endif %}
      {{ page.help }}
      {% if action_links %}
          <ul class="action-links">
              {{ action_links }}
          </ul>
      {% endif %}
      <main class="content"  role="main">
          {{ title_prefix }}
          {% if title %}
              <h1 class="title" class="page-title">
                  {{ title }}
              </h1>
          {% endif %}
          {{ title_suffix }}
          {{ page.content }}
      </main>
    </div>
    <footer id="footer">
      {% if page.footer %}
        {{ page.footer }}
      {% endif %}
      <div class="social-links">
        <a class="social" href="https://twitter.com/aaronfroehlich"><i class="fa fa-twitter"></i></a>
        <a class="social" href="https://github.com/a-fro "><i class="fa fa-github-alt"></i></a>
        <a class="social"  href="http://www.linkedin.com/in/aaronfroehlich"><i class="fa fa-linkedin-square"></i></a>
      </div>
    </footer>
  </div>
</div>
